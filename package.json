{
  "name": "blog-api",
  "version": "1.0.0",
  "description": "Express API for blog management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:clean": "npm run db:seed:undo && npm run db:migrate:undo:all",
    "prerailway:deploy": "npm run db:clean",
    "railway:deploy": "npm run db:setup && npm start"
  },
  "keywords": ["express", "blog", "api"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2"
  }
}
